{
  "cloud": {
    "currentProfile": "AzureCloud",
    "credentialPrecedence": ["AzureCLI", "AzurePowerShell"]
  },
  "analyzers": {
    "core": {
      "enabled": true,
      "verbose": true,
      "rules": {
        "no-unused-params": {
          "level": "error"
        },
        "no-unused-vars": {
          "level": "error"
        },
        "no-unused-existing-resources": {
          "level": "error"
        },
        "prefer-interpolation": {
          "level": "error"
        },
        "secure-parameter-default": {
          "level": "error"
        },
        "simplify-interpolation": {
          "level": "error"
        },
        "no-hardcoded-env-urls": {
          "level": "error"
        }
      }
    }
  }
}
